<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.MuseumQuests">

    <application
            android:allowBackup="true"
            android:icon="@drawable/mueseum"
            android:label="@string/app_name"
            android:roundIcon="@drawable/mueseum"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity
                android:screenOrientation="portrait"
                android:name=".NewQuestionActivity"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
                android:screenOrientation="portrait"
                android:name=".NewQuestActivity"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
                android:screenOrientation="portrait"
                android:name=".Sign_up">
        </activity>
        <activity
                android:screenOrientation="portrait" android:name=".Result">
        </activity>
        <activity android:screenOrientation="portrait" android:name=".Question">
        </activity>
        <activity android:screenOrientation="portrait" android:name=".QuestInfo">
        </activity>
        <activity  android:screenOrientation="portrait" android:name=".QuestList">
        </activity>
        <activity android:screenOrientation="portrait" android:name=".Home">
        </activity>
        <activity android:screenOrientation="portrait" android:name=".Start">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>